#include <stdarg.h>
#include <unistd.h>

void	print_conversion(const char *format, va_list ap)
{
	if (*format == 'c')
		write(1,   ,1);
        if (*format == 's')
	if (*format == 'p')
	if (*format == 'd')
	if (*format == 'i')
	if (*format == 'u')
	if (*format == 'x')
	if (*format == 'X')
	if (*format == '%')
		write(1, "%", 1);

}

void	check_format(const char *format, va_list ap)
{
	while (*format)
	{
		if (*format == %)
		{
			format++;
			print_conversion(format, ap);
			ap++:
		}
		else
			write(1,format, 1);
		format++;
	}
}

int	ft_prntf(const char *format, ...)
{
	va_list ap;
	int	i;
	
	va_start(ap, format);
	check_format(format, ap);
	va_end(ap);
	return 
}
 
